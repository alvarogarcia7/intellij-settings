<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'⌘F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'GameApplication'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'GameApplication'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'GameLoopTest'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'GameLoopTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'GameApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'GameApplication'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>