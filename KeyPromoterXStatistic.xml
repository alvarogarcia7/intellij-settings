          <Description>Remove Module</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Replace in Path...">
        <Item>
          <IdeaActionID>ReplaceInPath</IdeaActionID>
          <ShortCut>'⇧⌘R'</ShortCut>
          <Description>Replace in Path...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'GameApplication'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'GameApplication'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'GameLoopTest'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'GameLoopTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
