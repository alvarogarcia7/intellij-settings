<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add to VCS">
        <Item>
          <IdeaActionID>ChangesView.AddUnversioned.From.Dialog</IdeaActionID>
          <ShortCut>'⌥⌘A'</ShortCut>
          <Description>Add to VCS</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'⌘F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Message History">
        <Item>
          <IdeaActionID>Vcs.ShowMessageHistory</IdeaActionID>
          <ShortCut>'⌃M' or '⌘E'</ShortCut>
          <Description>Commit Message History</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Compare Files">
        <Item>
          <IdeaActionID>CompareTwoFiles</IdeaActionID>
          <ShortCut>'⌘D'</ShortCut>
          <Description>Compare Files</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>22</Count>
        </Item>
      </Statistic>
